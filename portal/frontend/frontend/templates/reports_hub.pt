<metal:main use-macro="load: main2.pt">

	<div metal:fill-slot="additional-css">
		<link rel="stylesheet" type="text/css" href="${static_url}css/reports_hub/reports_hub.css" />
		<script type="text/javascript" src="${static_url}js/reports_hub/init.js" defer></script>
	</div>

	<div metal:fill-slot="content">
		<p>(NOTE: The contents of these reports are updated hourly, on the 00, and takes a few minutes each time. Also note that this is a screen version of the final product, so some minor details may differ. In particular, attendance, may not be exactly correct.)
			<div class="ui-font">Enter any part of the name:</div>
			<div class="area">
				<input id="userEnter" type="text" name="userEnter" data-autocomplete-source="${auto_complete_source}" /><button id="userSubmit">Check out report</button><input type="hidden" id="studentID" />
			</div>
		</p>

	</div>

</metal:main>